# SnapRate Frontend Staging Environment Configuration

# API Configuration
# Base URL for the backend API in staging
VITE_API_BASE_URL=https://api-staging.snaprate.example.com

# Environment
# Staging environment setting
VITE_NODE_ENV=staging

# Feature flags
# Enable Progressive Web App features in staging
VITE_ENABLE_PWA=true
# Enable analytics tracking in staging
VITE_ENABLE_ANALYTICS=true
# Enable advanced AI features in staging for testing
VITE_ENABLE_ADVANCED_AI=true

# Performance settings
# Moderate image quality for staging
VITE_IMAGE_COMPRESSION_QUALITY=0.85
# Moderate maximum image size for staging (8MB)
VITE_MAX_IMAGE_SIZE=8388608
# Enable image optimization
VITE_OPTIMIZE_IMAGES=true
# Moderate cache timeout for staging (1 hour)
VITE_CACHE_TIMEOUT=3600

# Error reporting
# Enable error reporting in staging
VITE_ERROR_REPORTING_ENABLED=true
# Error reporting service URL for staging
VITE_ERROR_REPORTING_URL=https://monitoring-staging.snaprate.example.com/api/errors

# Staging settings
# Enable debug logging in staging for troubleshooting
VITE_ENABLE_DEBUG_LOGGING=true
# Disable mock API in staging
VITE_USE_MOCK_API=false